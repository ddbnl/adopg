{
    "manifestVersion": 1,
    "id": "azure-devops-policy-guard",
    "publisher": "deBoerICT",
    "version": "1.0.0.51",
    "name": "Azure Devops Policy Guard",
    "description": "Azure DevOps extension sample",
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ],
    "baseUri": "https://localhost:3000"
}
